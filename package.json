{
  "author": "Erlend Oftedal <erlend@oftedal.no>",
  "name": "retire-site-scanner",
  "version": "1.5.0",
  "license": "Apache-2.0",
  "description": "A scanner for checking a web site using retire.js",
  "main": "dist/index.js",
  "repository": {
		"type": "git",
		"url": "https://github.com/RetireJS/retire-site-scanner.git"
	},
  "bin": {
    "retire-site-scanner": "dist/index.js"
  },
  "scripts": {
    "start": "ts-node src/index",
    "build": "tsc --build",
    "watch": "tsc --watch",
    "typecheck": "tsc --noEmit",
    "check": "npm run lint && npm run typecheck",
    "test": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha -r ts-node/register 'tests/**/*.ts'",
    "lint": "eslint . --ext .js,.ts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "cacheable-lookup": "^7.0.0",
    "puppeteer": "^22.4.0",
    "retire": "^5.1.4",
    "source-map": "^0.7.4"
  },
  "devDependencies": {
    "@types/chai": "^4.3.4",
    "@types/mocha": "^10.0.0",
    "@types/node": "^18.11.9",
    "@rushstack/eslint-patch": "^1.2.0",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "chai": "^4.4.1",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jsonschema": "^1.4.1",
    "mocha": "^10.1.0",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.4.2"
  },
  "keywords": [
    "audit",
    "scanner",
    "security",
    "sbom",
    "sbom-tool",
    "sbom-generator",
    "cli"
  ],
  "files": [
    "dist/**/*"
  ]
}
